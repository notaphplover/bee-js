  "devDependencies": {
    "@types/jasmine": "^3.3.13",
    "coveralls": "^3.0.5",
    "jasmine": "^3.4.0",
    "nyc": "^14.1.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "eventemitter3": "^4.0.0"
  }
}
